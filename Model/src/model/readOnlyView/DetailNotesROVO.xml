  <SQLQuery><![CDATA[SELECT distinct  
    nmei.ID_NOTE_MODE_ENS_INTER,  
    nmei.ID_CALENDRIER_DELIBERATION,  
    nmei.NOTE,  
    mec.ID_MODE_CONTROLE_EC,  
    fec.ID_FILIERE_UE_SEMSTRE_EC,  
    mec.ID_PARCOURS_MAQUETTE_ANNEE,  
    tc.ID_TYPE_CONTROLE,  
    etd.NUMERO,  
    etd.ID_ETUDIANT,  
    pers.PRENOMS || ' ' || pers.NOM || ' ' || DECODE(pers.ID_SEXE, 1, 'Né le ', 'Née le ') || TO_CHAR(DATE_NAISSANCE, 'DD-MM-YYYY') || ' à ' || pers.LIEU_NAISSANCE DateLieuNaiss,  
    fus.ID_FILIERE_UE_SEMSTRE,  
    fus.CODIFICATION,  
    UE.LIBELLE_LONG UniteEns,  
    EC.LIBELLE_LONG || DECODE(e_td.LIBELLE_LONG, NULL, NULL, '(' || e_td.LIBELLE_LONG || ')') ElementConst,  
    tc.LIBELLE_LONG TypeCntrl,  
    nmei.ID_TYPE_VERROU,  
    ips.ID_INSCRIPTION_PED_SEMESTRE,  
    EC.A_TD A_TD  
FROM 
    ((((((((((((PERSONNES pers JOIN ETUDIANTS etd ON pers.ID_PERSONNE = etd.ID_PERSONNE) JOIN INSCRIPTIONS_ADMIN ia ON etd.ID_ETUDIANT = ia.ID_ETUDIANT) JOIN INSCRIPTIONS_PEDAGOGIQUES ip ON ia.ID_INSCRIPTION_ADMIN = ip.ID_INSCRIPTION_ADMIN) JOIN INSCRIPTION_PED_SEMESTRE ips ON ip.ID_INSCRIPTION_PEDAGOGIQUE = ips.ID_INSCRIPTION_PEDAGOGIQUE) JOIN INSCRIPTION_PED_SEM_UE iue ON ips.ID_INSCRIPTION_PED_SEMESTRE = iue.ID_INSCRIPTION_PED_SEMESTRE) JOIN FILIERE_UE_SEMSTRE fus ON iue.ID_FILIERE_UE_SEMSTRE = fus.ID_FILIERE_UE_SEMSTRE AND ips.ID_SEMESTRE = fus.ID_SEMESTRE) JOIN UE ON fus.ID_UE = UE.ID_UE) JOIN FILIERE_UE_SEMSTRE_EC fec ON fus.ID_FILIERE_UE_SEMSTRE = fec.ID_FILIERE_UE_SEMSTRE) JOIN EC ON fec.ID_EC = EC.ID_EC) JOIN MODE_CONTROLE_EC mec ON fec.ID_FILIERE_UE_SEMSTRE_EC = mec.ID_FILIERE_UE_SEMSTRE_EC AND ip.ID_PARCOURS_MAQUET_ANNEE = mec.ID_PARCOURS_MAQUETTE_ANNEE) JOIN TYPE_CONTROLE tc ON mec.ID_TYPE_CONTROLE = tc.ID_TYPE_CONTROLE) JOIN NOTES_MODE_ENSEIGNEMENT_INTER nmei ON mec.ID_MODE_CONTROLE_EC = nmei.ID_MODE_CONTROLE_EC AND iue.ID_INSCRIPTION_PED_SEM_UE = nmei.ID_INSCRIPTION_PED_SEM_UE AND nmei.ID_TYPE_CONTROLE = tc.ID_TYPE_CONTROLE) LEFT JOIN ( 
        SELECT distinct  
            mec.ID_MODE_CONTROLE_EC,  
            EC.LIBELLE_LONG  
        FROM  
            (MODE_CONTROLE_EC mec JOIN FILIERE_UE_SEMSTRE_EC fec ON mec.EC_TD = fec.ID_FILIERE_UE_SEMSTRE_EC) JOIN EC ON fec.ID_EC = EC.ID_EC 
    ) e_td ON mec.ID_MODE_CONTROLE_EC = e_td.ID_MODE_CONTROLE_EC  
WHERE  
    etd.NUMERO = :p_numero AND ip.ID_PARCOURS_MAQUET_ANNEE = :p_pma AND nmei.ID_CALENDRIER_DELIBERATION = :p_calendrier AND nmei.VALIDE = 1  
ORDER BY  
    fus.CODIFICATION,  
    fec.ID_FILIERE_UE_SEMSTRE_EC,  
    tc.ID_TYPE_CONTROLE]]></SQLQuery>
