  <Variable
    Name="id_ped"
    Kind="where"
    Type="java.lang.Long"/>
  <SQLQuery><![CDATA[select       
ip.cout_formation,       
ip.etat_inscr_etat_inscr_id ,       
ip.id_inscription_admin ,       
ip.id_inscription_pedagogique ,       
ip.id_parcours_maquet_annee ,       
ip.ins_en_ligne ,      
nf.id_niveau_formation ,       
nf.libelle_court as lib_court_niv_form ,       
nf.libelle_long as lib_niv_form,       
nf.id_mode_paiement ,       
f.ecolage ,       
f.id_formation ,       
f.id_historiques_structure ,       
f.libelle_court as lib_court_form,       
f.libelle_long as lib_form,    
ia.id_etudiant,    
p.id_pays_nationalite,  
f.payante        
from inscriptions_pedagogiques ip      
join parcours_maquette_annee pma on ip.id_parcours_maquet_annee = pma.id_parcours_maquet_annee      
join niveaux_formation_parcours nfp on nfp.id_niveau_formation_parcours = pma.id_niveau_formation_parcours      
join niveau_formation_cohortes nfc on nfc.id_niveau_formation_cohorte = nfp.id_niveau_formation_cohorte      
join niveaux_formations nf on nf.id_niveau_formation = nfc.id_niveau_formation      
join formations f on f.id_formation = nf.id_formation      
join inscriptions_admin ia on ia.id_inscription_admin = ip.id_inscription_admin  
join etudiants e on e.id_etudiant = ia.id_etudiant  
join personnes p on p.id_personne = e.id_personne  
join pays pa on pa.id_pays = p.id_pays_nationalite     
      
where ip.id_inscription_pedagogique=:id_ped      
and f.payante = 1 and f.ecolage = 1]]></SQLQuery>
