  <SQLQuery><![CDATA[Select Distinct nf.ID_NIVEAU_FORMATION,unf.ID_UTILISATEUR,nf.LIBELLE_LONG,str.ID_STRUCTURE       
from  STRUCTURES str    
JOIN HISTORIQUES_STRUCTURES hs on str.ID_STRUCTURE=hs.ID_STRUCTURES       
JOIN FORMATIONS fr on hs.ID_HISTORIQUES_STRUCTURE=fr.ID_HISTORIQUES_STRUCTURE       
Join NIVEAUX_FORMATIONS nf on fr.ID_FORMATION=nf.ID_FORMATION       
JOIN UTILISATEUR_NIVEAUX_FORMATIONS unf on unf.ID_NIVEAU_FORMATION = nf.ID_NIVEAU_FORMATION       
Join UTILISATEURS uti on unf.ID_UTILISATEUR=uti.ID_UTILISATEUR]]></SQLQuery>
