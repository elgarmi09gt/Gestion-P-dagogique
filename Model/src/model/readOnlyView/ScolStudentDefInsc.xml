  <SQLQuery><![CDATA[select distinct  fa.code_formation,ns.code_niv_sec,etu.numero, etu.numero_table,fa.LIBELLE_LONG Formation,fa.CODE_GRADE_DIPLOME,fa.TYPE_FORMATION,ns.LIBELLE_LONG NiveauFormation,                               
ins_adm.ETAT_INSCR_ETAT_INSCR_ID  etat_inscription, ins_adm.ins_en_ligne,ins_adm.horaire_td_code,ins_adm.statut_code, ips.code_option, ins_adm.operateur,                                
ins_adm.type_convention, etu.BOURSIER, etu.NATURE_BOURSE, ins_adm.motif, ins_adm.redoublement, ins_adm.quittance, ins_adm.tarif, ins_adm.nb_insc, ins_adm.permet_double_inscription,                                
ins_adm.cartetiree, ins_adm.ordre_inscription, ins_adm.code_a_conserver, ins_adm.date_edition_carte,etu.ORG_BOURSE_ORG_BOURSE_ID,ips.ins_ped_valide, ips.passe_exam, ips.cumul_credit, ips.dette_credit          
,Decode(crt.type_cohorte,null,1,crt.type_cohorte) type_cohorte,ins_adm.cohorte,ips.code_semestre                   
from etudiants@scolProdref etu          
Join scol_inscription_ped_sem@scolProdref ips On ips.etudiant_numero=etu.numero          
Join inscriptions@scolProdref ins_adm On ips.code_niv_sec=ins_adm.code_niv_sec           
and ins_adm.ETUDIANT_NUMERO=etu.numero and ins_adm.ETUDIANT_NUMERO=ips.etudiant_numero                
JOIN NIVEAUX_SECTIONS@scolProdref ns On ips.code_niv_sec=ns.CODE_NIV_SEC                               
AND ns.CODE_NIV_SEC=ins_adm.code_niv_sec          
JOIN NIVEAUX_SECTION_FORMATIONS@scolProdref nsf On ns.CODE_NIV_SEC = nsf.CODE_NIV_SEC          
JOIN Formation@scolProdref fr On  nsf.CODE_FORMATION = fr.CODE_FORMATION          
JOIN FORMATION_ACC_FORMATION@scolProdref faf On faf.ID_FORMATION = fr.CODE_FORMATION          
JOIN FORMATION_ACCREDITE@scolProdref fa On fa.CODE_FORMATION=faf.ID_FORMATION_ACC          
Left Join cohorte@scolProdref crt On crt.code_niv_sec=ns.code_niv_sec and crt.code_cohorte=ins_adm.cohorte          
where ins_adm.code_niv_sec=:niv_form          
AND ins_adm.AN_UNIVERS_AN_UNIVERS_ID=:an_id          
AND ins_adm.ETAT_INSCR_ETAT_INSCR_ID=1          
AND etu.NON_AUTORISE = 0          
AND fa.code_formation=:fr_id          
ORDER BY etu.numero]]></SQLQuery>
