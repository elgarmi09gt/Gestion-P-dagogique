                                <af:column sortable="true" headerText="Resultat Semestre" id="c7" align="center"
                                           inlineStyle="font-size:medium; text-align:center; font-weight:bold;"
                                           filterable="true">
                                           
                                           <af:column sortable="false" headerText="Moyenne" id="c9" align="center"
                                                       width="52"
                                                       inlineStyle="font-size:medium; text-align:center; font-weight:bold;border-left: solid 2px;">
                                                <af:outputText value="#{sessionScope.isdelibsem == '0' ? '-' :  row.getNote() }" id="ot7"
