  <SQLQuery><![CDATA[select distinct a.id_etudiant ,a.id_annees_univers , ip.etat_inscr_etat_inscr_id , ip.id_inscription_pedagogique , ip.id_parcours_maquet_annee , ip.ins_en_ligne, ips.cumul_credit , ips.dette_credit , ips.id_inscription_ped_semestre , ips.id_inscription_pedagogique , ips.id_semestre                       
from inscriptions_admin a                   
join inscriptions_pedagogiques ip on ip.id_inscription_admin = a.id_inscription_admin                  
join parcours_maquette_annee pma on pma.id_parcours_maquet_annee = ip.id_parcours_maquet_annee   and pma.id_annees_univers = a.id_annees_univers 
join inscription_ped_semestre ips on ips.id_inscription_pedagogique = ip.id_inscription_pedagogique   
JOIN etudiants e on e.id_etudiant = a.id_etudiant   
where  e.id_personne=:idpers    
and pma.id_parcours_maquet_annee=:id_parc 
and a.id_annees_univers =:id_annee 
and ips.id_semestre=:id_sem]]></SQLQuery>
