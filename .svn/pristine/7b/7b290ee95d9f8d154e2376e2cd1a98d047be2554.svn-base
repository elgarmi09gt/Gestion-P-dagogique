<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="list-sessions">
    <default-activity>listsessions</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__1">
      <managed-bean-name>Sessionsbean</managed-bean-name>
      <managed-bean-class>view.bean.sessions.SessionsBean</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <view id="listsessions">
      <page>/parametrage/sessions/listsessions.jsff</page>
    </view>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
