  <SQLQuery><![CDATA[select f.id_formation , f.libelle_court , f.libelle_long , hs.id_historiques_structure , s.id_structure , s.libelle_long as lib_strct,au.id_annees_univers         
from structures s       
join historiques_structures hs on s.id_structure = hs.id_structures   
join annees_univers au on au.id_annees_univers=:id_annee  
join formations f on hs.id_historiques_structure = f.id_historiques_structure       
join (select id_structures from historiques_structures where id_historiques_structure=:id_hs)  str  on s.id_structure=str.id_structures   
where f.payante = 1 and f.ecolage = 1]]></SQLQuery>
