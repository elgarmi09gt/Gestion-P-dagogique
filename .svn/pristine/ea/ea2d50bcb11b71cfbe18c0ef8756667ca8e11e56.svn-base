<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="list-bourses">
    <default-activity>listbourses</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__1">
      <managed-bean-name>Boursesbean</managed-bean-name>
      <managed-bean-class>bourses.BoursesBean</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <view id="listbourses">
      <page>/inscription/bourses/listbourses.jsff</page>
    </view>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
