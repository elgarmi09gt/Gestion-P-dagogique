  <SQLQuery><![CDATA[select e.numero , p.nom , p.prenoms , p.date_naissance , p.adresse, p.piece_identification,gte.id_groupe_tp_td_etudiant,ia.id_annees_univers ,gte.id_groupe_tp_td       
from groupe_tp_td_etudiants gte        
join inscriptions_pedagogiques ip on gte.id_inscription_pedagogique = ip.id_inscription_pedagogique          
join inscriptions_admin ia on ia.id_inscription_admin = ip.id_inscription_admin        
join etudiants e on e.id_etudiant  = ia.id_etudiant        
join personnes p on e.id_personne = p.id_personne        
where ia.id_annees_univers=:id_annee        
and id_groupe_tp_td=:id_groupe]]></SQLQuery>
