                      shortDesc="#{bindings.UtiModifie.hints.tooltip}" id="it14">
          <f:validator binding="#{bindings.UtiModifie.validator}"/>
          <af:convertNumber groupingUsed="false" pattern="#{bindings.UtiModifie.format}"/>
        </af:inputText-->
