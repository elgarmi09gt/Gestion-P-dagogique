    public void OnReopenInterAction(DialogEvent dialogEvent) {
        Outcome outcome = dialogEvent.getOutcome();
        DCIteratorBinding iterUe = (DCIteratorBinding) getBindings().get("UeEvalROIterator");
        Row currentUeRow = iterUe.getCurrentRow();
        System.out.println("Fil_Ue : "+currentUeRow.getAttribute("IdFiliereUeSemstre").toString());
        DCIteratorBinding iterEc = (DCIteratorBinding) getBindings().get("EcEvalROIterator");
        Row currentEcRow = iterEc.getCurrentRow();
        System.out.println("Fil_Ec : "+currentEcRow.getAttribute("IdFiliereUeSemstreEc").toString());
        DCIteratorBinding iterTc = (DCIteratorBinding) getBindings().get("EcEvalTypeCtrlROIterator");
        Row currentTcRow = iterTc.getCurrentRow();
        System.out.println("Type controle : "+currentTcRow.getAttribute("IdTypeControle").toString());
        DCIteratorBinding iterMc = (DCIteratorBinding) getBindings().get("EcEvalTypeCtrlModeCtrlROIterator");
        Row currentMcRow = iterMc.getCurrentRow();
        System.out.println("Mode Controle : "+currentMcRow.getAttribute("IdModeControleEc").toString());
        this.getPopupConfirmOpenInter().hide();
        if (outcome == Outcome.yes) {
                    OperationBinding openSaisieInter = getBindings().getOperationBinding("reouvrirSaisieNotesInter");
            openSaisieInter.getParamsMap().put("parcours", Long.parseLong(getParcours()));
            openSaisieInter.getParamsMap()
                .put("fil_ue", Long.parseLong(currentUeRow.getAttribute("IdFiliereUeSemstre").toString()));
            openSaisieInter.getParamsMap()
                .put("fil_sem_ec", Long.parseLong(currentEcRow.getAttribute("IdFiliereUeSemstreEc").toString()));
            openSaisieInter.getParamsMap()
                .put("type_control", Long.parseLong(currentTcRow.getAttribute("IdTypeControle").toString()));
            openSaisieInter.getParamsMap()
                .put("mode_control", Long.parseLong(currentMcRow.getAttribute("IdModeControleEc").toString()));
            openSaisieInter.getParamsMap().put("calendrier",getCal());
            openSaisieInter.getParamsMap().put("utilisateur", getUtilisateur());

            Integer result = (Integer) openSaisieInter.execute();
            System.out.println("Is open saisie inter : "+result);
            AdfFacesContext.getCurrentInstance().addPartialTarget(this.getTableSaisieNotesInter()); 
            AdfFacesContext.getCurrentInstance().addPartialTarget(this.getPanelbtngroupe()); 
            //RichPopup popup = this.getPopupSuccessOpen();
            //RichPopup.PopupHints hints = new RichPopup.PopupHints();
            //empty hints renders dialog in center of screen
            //popup.show(hints);
        } 
    }

