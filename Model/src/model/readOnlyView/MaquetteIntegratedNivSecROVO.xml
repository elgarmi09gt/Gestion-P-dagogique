  <SQLQuery><![CDATA[Select Distinct fue.ID_FILIERE_UE_SEMSTRE,pma.ID_NIVEAU_FORMATION_PARCOURS,pma.ID_ANNEES_UNIVERS,fue.ID_MAQUETTE ,fue.ID_UE,fue.ID_SEMESTRE,fue.ID_CATEGORIE_UE,fue.CODIFICATION,                            
fue.CREDIT ,fue.COEFFICIENT ,fue.MOYENNE_VALIDATION,fue.MOYENNE_ELIMINATOIRE,fue.ID_NATURE_UE,fue.COMPENSABLE,                            
fue.ANCIEN_CODE,fue.IS_SOUTENANCE,UE.LIBELLE_LONG,mq.ID_STRUCTURE                  
FROM PARCOURS_MAQUETTE_ANNEE pma                  
JOIN MAQUETTES mq on pma.ID_MAQUETTE=mq.ID_MAQUETTE                  
JOIN FILIERE_UE_SEMSTRE fue on fue.ID_MAQUETTE=mq.ID_MAQUETTE                  
JOIN UE On fue.ID_UE=UE.ID_UE                  
JOIN MAQUETTEVALIDEE mv on mv.ID_MAQUETTE=fue.ID_MAQUETTE    
AND mv.ID_ANNEE=pma.ID_ANNEES_UNIVERS and mv.ID_NIVEAU_FORMATION_PARCOURS=pma.ID_NIVEAU_FORMATION_PARCOURS                              
ORDER BY fue.ID_SEMESTRE,fue.CODIFICATION]]></SQLQuery>
