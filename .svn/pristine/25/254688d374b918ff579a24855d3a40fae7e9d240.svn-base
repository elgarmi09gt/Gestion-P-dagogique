        for(int id_semestre = 0; id_semestre < les_semestre.size(); id_semestre++){
            
            OperationBinding op_ue_insc = getBindings().getOperationBinding("getUeInsc");
            op_ue_insc.getParamsMap().put("id_parcours_maq",  parc);
