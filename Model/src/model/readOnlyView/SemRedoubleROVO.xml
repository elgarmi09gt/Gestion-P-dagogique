  <SQLQuery><![CDATA[select au.id_annees_univers ,   
au.libelle_long lib_annee, ia.id_etudiant,  
ia.id_inscription_admin,  
ia.insc_ligne,   
ip.etat_inscr_etat_inscr_id,   
ip.id_inscription_pedagogique,  
ip.id_parcours_maquet_annee ,  
ips.cumul_credit, ips.dette_credit, ips.id_inscription_ped_semestre, ips.id_semestre , ips.ins_ped_valide   
    
from inscriptions_admin ia    
join inscriptions_pedagogiques ip on ia.id_inscription_admin = ip.id_inscription_admin   
join inscription_ped_semestre ips on ips.id_inscription_pedagogique = ip.id_inscription_pedagogique  
join annees_univers au on au.id_annees_univers = ia.id_annees_univers    
    
where ip.id_parcours_maquet_annee=:id_parc_maq and  ia.id_etudiant=:id_etud and id_semestre =:id_sem and id_inscription_ped_semestre in (  
select rs.id_inscription_ped_semestre from resultats_semestre rs where (rs.credit = 30 or rs.resultat = 2))]]></SQLQuery>
