                      id="it8">
          <f:validator binding="#{bindings.UtiCree.validator}"/>
          <af:convertNumber groupingUsed="false" pattern="#{bindings.UtiCree.format}"/>
        </af:inputText-->
      </af:panelFormLayout>
    </af:dialog>
  </af:popup>
