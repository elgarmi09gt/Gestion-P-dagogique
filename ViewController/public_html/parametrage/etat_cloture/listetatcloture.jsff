                                    <af:showPopupBehavior popupId=":::popupDelete-etat-cloture"/>
                                </af:commandMenuItem>
                            </af:menu>
                        </af:popup>
                    </f:facet>
                </af:table>
                <f:facet name="statusbar"/>
