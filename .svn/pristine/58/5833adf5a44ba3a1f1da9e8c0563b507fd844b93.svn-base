  <SQLQuery><![CDATA[select p.annule ,         
p.complet ,         
p.date_paiement ,         
p.date_valeur ,         
p.etat ,         
p.id_annees_univers ,         
p.id_echelon_paiement ,         
p.id_etudiant ,         
p.id_inscription_pedagogique ,         
p.id_formation ,         
p.id_paiement ,         
p.id_type_paiement ,         
p.montant ,         
p.montant_exoneration ,         
p.net_a_payer ,         
p.paye ,         
p.reparti ,         
p.ref_exo ,         
p.numero_cheque ,         
p.numero_virement, ep.ordre,        
ep.id_mois , ep.libelle_echelon , ep.niv_form ,tp.libelle_long as lib_type_paie         
from paiements p         
join type_paiements tp on tp.id_type_paiement = p.id_type_paiement         
join echelons_paiement ep on ep.id_echelon_paiement = p.id_echelon_paiement        
where p.paye=1    
order by ep.ordre desc]]></SQLQuery>
