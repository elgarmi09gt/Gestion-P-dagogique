            op_ue_insc.getParamsMap().put("id_insc_ped",  id_insc_ped);        
            op_ue_insc.execute();
            
            DCIteratorBinding iter_ue_insc = (DCIteratorBinding) BindingContext.getCurrent().getCurrentBindingsEntry().get("UeInscIterator");        
