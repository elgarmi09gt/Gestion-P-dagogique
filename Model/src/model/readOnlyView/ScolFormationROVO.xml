  <SQLQuery><![CDATA[SELECT DISTINCT fa.CODE_FORMATION ,fa.LIBELLE_LONG,fa.LIBELLE_COURT ,fa.CODE_DEPARTEMENT,fa.PAYANTE ,Decode(fa.CODE_GRADE_DIPLOME,null,fr.code_grade_diplome,fa.CODE_GRADE_DIPLOME)CODE_GRADE_DIPLOME,  
Decode(fa.CODE_MENTION,null,fr.code_mention,fa.CODE_MENTION)CODE_MENTION,Decode(fa.OUVERT,null,fr.ouvert,fa.OUVERT)OUVERT,fa.RESPONSABLE,       
  Decode(fa.VALIDE,null,fr.valide,fa.VALIDE)VALIDE,Decode(fa.TYPE_FORMATION,null,fr.type_formation,fa.TYPE_FORMATION)TYPE_FORMATION,Decode(fa.PRESENTIELLE,null,fr.presentielle,fa.PRESENTIELLE)PRESENTIELLE,  
  Decode(fa.PROFESSIONNALISANTE,null,fr.professionnalisante,fa.PROFESSIONNALISANTE)PROFESSIONNALISANTE,Decode(fa.CYCLE_CODE,null,fr.CYCLE_CODE,fa.CYCLE_CODE)CYCLE_CODE,  
  Decode(fa.FORMATION_RECONNUE,null,fr.FORMATION_RECONNUE,fa.FORMATION_RECONNUE)FORMATION_RECONNUE,fa.JUSTIFICATION_PROGRAMME,fa.ORGANISATION_PROGRAMME,       
  fa.PROFIL_ACADEMIQUE,fa.PROFIL_PROFESSIONNEL,fa.ID_RESPONSABLE,fa.OBSERVATIONS,fa.ID_SPECIALITE,fa.ID_OPTION,fa.INTITULE_PARCOURS, fr.CODE_ED CODE_EDOC       
FROM FORMATION_ACCREDITE@scolProdref fa       
JOIN FORMATION_ACC_FORMATION@scolProdref faf ON fa.CODE_FORMATION=faf.ID_FORMATION_ACC       
JOIN Formation@scolProdref fr ON faf.ID_FORMATION = fr.CODE_FORMATION   
ORDER BY fa.LIBELLE_LONG]]></SQLQuery>
