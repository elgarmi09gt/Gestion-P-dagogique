  <SQLQuery><![CDATA[select ip.id_inscription_pedagogique ,ip.id_parcours_maquet_annee , ip.etat_inscr_etat_inscr_id , e.id_personne , e.id_etudiant , e.numero , ra.credit , ra.id_resultat_annuel , ra.note , ra.resultat, au.libelle_long           
    from inscriptions_admin ia                       
    join inscriptions_pedagogiques ip on ip.id_inscription_admin = ia.id_inscription_admin                                           
    join etudiants e  on e.id_etudiant = ia.id_etudiant       
    join resultats_annuels ra on ra.id_inscription_pedagogique = ip.id_inscription_pedagogique   
    join annees_univers au on au.id_annees_univers = ia.id_annees_univers  
          
where e.id_personne=:idpers and ip.id_parcours_maquet_annee=:id_parcours and (ra.note >= 10 or ra.resultat = 1 or ra.resultat = 5)]]></SQLQuery>
