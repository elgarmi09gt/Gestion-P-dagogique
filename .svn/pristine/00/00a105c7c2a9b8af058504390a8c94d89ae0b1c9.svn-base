  <SQLQuery><![CDATA[select distinct r.id_repartition ,            
r.id_structure,            
r.libelle_long as lib_rep ,           
c.id_compte ,            
c.id_formation ,            
c.id_historiques_structure ,             
c.id_type_compte ,            
c.numero_compte ,            
cr.pourcentage ,            
fr.id_formation as id_form_frep,            
sc.id_annees_univers ,             
sc.id_solde_compte ,            
sc.solde_actuel ,            
sc.solde_initial ,               
tc.libelle_long as lib_type_cmpte ,    
s.libelle_long as lib_strc    
from repartition r           
join formation_repartition fr on fr.id_repartition = r.id_repartition          
join cle_repartition cr on cr.id_repartition = r.id_repartition            
join type_compte tc on tc.id_type_compte = cr.id_type_compte           
join comptes c on c.id_type_compte = tc.id_type_compte           
join soldes_comptes sc on sc.id_compte = c.id_compte and sc.id_annees_univers = c.id_annees_univers and c.id_annees_univers = fr.id_annees_univers           
join formations f on f.id_formation = fr.id_formation                  
join (select id_structures from historiques_structures where id_historiques_structure=:id_hs)  str  on r.id_structure=str.id_structures              
join utilisateur_structures us on us.id_structure = str.id_structures         
join structures s on s.id_structure = str.id_structures     
    
where fr.id_annees_univers=:id_annee           
and us.id_utilisateur=:id_util        
and c.id_historiques_structure is null        
and c.id_formation is null]]></SQLQuery>
