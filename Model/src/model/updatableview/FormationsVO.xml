    ColumnType="VARCHAR2"
    AliasName="VIEW_ATTR"
    SQLType="VARCHAR"
    IsQueriable="false"
    Expression="Select pers.PRENOMS || ' '||pers.NOM Responsable 
from FORMATIONS fr 
join UTILISATEUR_FORMATIONS uf on uf.ID_FORMATION=fr.ID_FORMATION 
and uf.ID_FORMATION=FormationsEO.ID_FORMATION 
and uf.ROLE='RESPONSABLE FORMATION'
JOIN UTILISATEURS uti on uf.ID_UTILISATEUR=uti.ID_UTILISATEUR
JOIN PERSONNES pers on uti.ID_PERSONNE=pers.ID_PERSONNE"/>
