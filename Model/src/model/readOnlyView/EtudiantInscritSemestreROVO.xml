  <SQLQuery><![CDATA[SELECT               
    e.numero,p.prenoms||' '||p.nom as prenom_nom,p.date_naissance,p.lieu_naissance, 
    ips.ID_INSCRIPTION_PED_SEMESTRE, 
    e.id_etudiant               
FROM personnes p               
JOIN etudiants e on p.id_personne = e.id_personne                 
JOIN inscriptions_admin ia on e.id_etudiant = ia.id_etudiant               
JOIN inscriptions_pedagogiques ip on ia.id_inscription_admin = ip.id_inscription_admin               
JOIN inscription_ped_semestre ips on ip.id_inscription_pedagogique = ips.id_inscription_pedagogique]]></SQLQuery>
