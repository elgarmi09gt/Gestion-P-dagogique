  <SQLQuery><![CDATA[select  e.id_etudiant , e.id_personne,p.nom, p.prenoms,p.date_naissance,nationalite, s.id_suspension, e.numero ,  s.duree , s.date_fin , s.date_debut , s.motif_suspension, au.libelle_long as lib_annee,s.levee,s.uti_levee    
from suspension s           
JOIN etudiants e on e.id_etudiant = s.id_etudiant    
JOIN personnes p on p.id_personne = e.id_personne   
JOIN pays pa on pa.id_pays = p.id_pays_nationalite   
JOIN annees_univers au on s.id_annees_univers = au.id_annees_univers     
where s.levee = 0]]></SQLQuery>
