  <SQLQuery><![CDATA[select rc.id_resultats_cycles, rc.credit, rc.id_annees_univers , rc.id_formation_option , rc.id_formation_specialite , rc.id_session , rc.id_type_mention , rc.resultat , rc.note , f.id_cycle , f.id_formation , f.id_grades_formation , f.id_mention , f.libelle_long  ,c.libelle_long as lib_cycle,gf.libelle_long as lib_grade_form   
from resultats_cycles rc     
join formations f on f.id_formation = rc.id_formation     
join etudiants e on e.id_etudiant = rc.id_etudiant  
join cycles c on c.id_cycle = f.id_cycle 
join grades_formation gf on gf.id_grades_formation = f.id_grades_formation 
 
where e.id_personne=:id_pers     
and (f.id_cycle=:id_prem_cycle or  f.id_cycle=:id_deux_cycle)  
order by rc.id_resultats_cycles]]></SQLQuery>
