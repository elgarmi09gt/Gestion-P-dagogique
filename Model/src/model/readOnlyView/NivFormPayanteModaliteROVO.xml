  <SQLQuery><![CDATA[select      
m.id_modalite ,      
m.libelle_long ,      
m.mensualite ,  
m.mensualite_etr,  
m.cout_formation ,      
m.cout_formation_etr ,      
m.libelle_court,    
fm.annee_fin ,      
fm.duree ,      
fm.id_annees_univers ,      
au.libelle_long as lib_annee_univers,      
fm.id_formation ,      
fm.id_formation_modalite ,      
fm.annee_debut ,      
fm.id_mois_debut , mois.libelle_long as lib_mois_deb ,     
fm.id_mois_fin , mo.libelle_long as lib_mois_fin ,     
fm.id_niveau_form ,      
m.droit_ins_adm ,      
m.droit_ins_adm_etr ,      
m.droit_ins_ped ,      
m.droit_ins_ped_etr ,     
mois.libelle_long||' / '||fm.annee_debut as mois_annee_deb ,     
mo.libelle_long||' / '||fm.annee_fin as mois_annee_fin     
     
from modalites m     
join formations_modalites fm on m.id_modalite = fm.id_modalite     
join annees_univers au on au.id_annees_univers = fm.id_annees_univers     
join mois on mois.id_mois = fm.id_mois_debut     
join mois mo on mo.id_mois = fm.id_mois_fin]]></SQLQuery>
