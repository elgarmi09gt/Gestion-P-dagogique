                                                       width="52"
                                                       inlineStyle="font-size:medium; text-align:center; font-weight:bold;border-left: solid 2px;">
                                                <af:outputText value="#{sessionScope.isdelibsem == '0' ? '-' : row.getResultat()}" id="ot13"
                                                               inlineStyle="font-weight:bold;"/>
                                            </af:column>
                                    <!--af:forEach items="#{DeliberationSemestrielleBean.getResultSmstre(DeliberationSemestrielleBean.getInsPedSem(row.numero))}"
                                                var="res">
                                        <af:forEach items="#{res}" var="rs">
                                            <af:column sortable="false" headerText="#{rs.key}" id="c9" align="center"
                                                       width="52"
                                                       inlineStyle="font-size:medium; text-align:center; font-weight:bold;border-left: solid 2px;">
                                                <af:outputText value="#{rs.value}" id="ot7"
                                                               inlineStyle="font-weight:bold;"/>
                                            </af:column>
                                        </af:forEach>
                                    </af:forEach-->
                                </af:column>
                            </af:table>
