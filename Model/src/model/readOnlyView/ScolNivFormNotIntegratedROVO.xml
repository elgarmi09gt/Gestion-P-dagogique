  <SQLQuery><![CDATA[SELECT DISTINCT ns.CODE_NIV_SEC,fa.CODE_FORMATION,ns.LIBELLE_LONG,ns.LIBELLE_COURT,ns.A_SELECTION,ns.AUTORISATION_PERMISE,ns.PRESENTIELLE,ns.NBRE_INS_PERMISES,ns.DIPLOMANTE,                                     
        ns.OUVERT,fr.CODE_ED  CODE_EDOC,ns.UTILISE_EVALUATION,ns.TAG_SEMESTRIALISATION,ns.TAG_GROUPEMATIERE,ns.NIVEAU_CODE,crt.code cohorte,crt.nombre_etudiant,crt.actif,sec.code section_code                
        ,fa.ID_SPECIALITE,fa.ID_OPTION,Decode(fa.CODE_GRADE_DIPLOME,null,fr.code_grade_diplome,fa.CODE_GRADE_DIPLOME)CODE_GRADE_DIPLOME,resp.emailucad,resp.prenoms ||' '|| resp.nom Responsable,        
        resp.code_etablissement etablissement,resp.code_matricule        
FROM SECTIONS@scolProdref sec                         
JOIN NIVEAUX_SECTIONS@scolProdref ns on sec.code=ns.section_code                           
join  NIVEAUX_SECTION_FORMATIONS@scolProdref nsf ON ns.CODE_NIV_SEC = nsf.CODE_NIV_SEC                                       
join Formation@scolProdref fr ON nsf.CODE_FORMATION = fr.CODE_FORMATION                                      
join FORMATION_ACC_FORMATION@scolProdref faf ON faf.ID_FORMATION = fr.CODE_FORMATION                                      
join FORMATION_ACCREDITE@scolProdref fa ON faf.ID_FORMATION_ACC = fa.CODE_FORMATION                                  
Left JOIN NIVEAUX_FORMATIONS nf on ns.code_niv_sec=nf.ANCIEN_CODE                  
LEFT JOIN (                  
    Select crt.code_niv_sec,nombre_etudiant,actif,tc.code                  
    from COHORTE@scolProdref crt join TYPE_COHORTE@scolProdref tc on crt.type_cohorte=tc.code) crt                                     
    ON crt.code_niv_sec=ns.CODE_NIV_SEC                  
Left JOIN (                                         
  Select distinct prf.code_matricule,ag.emailucad, prf.prenoms, prf.nom, code_etablissement, prof_id FROM PROFESSEURS@scolProdref prf                                          
  JOIN PROFESSEURS_ETABLISSEMENTS@scolProdref prfe On prf.prof_id=prfe.no_emp                                        
  JOIN v_agents@GRH_PAIE_LINK ag ON prf.code_matricule=ag.code_matricule                                       
  where prf.code_matricule is not null                                        
  )resp ON resp.prof_id=fa.id_responsable              
WHERE nf.ANCIEN_CODE is null          
ORDER BY ns.LIBELLE_LONG]]></SQLQuery>
