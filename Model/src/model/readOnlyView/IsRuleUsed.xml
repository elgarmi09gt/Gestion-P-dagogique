<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="IsRuleUsed"
  Version="12.2.1.12.24"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.readOnlyView.IsRuleUsed_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="id_r"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="id_an"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="id_nfp"
    Kind="where"
    Type="java.lang.Long"/>
  <Variable
    Name="id_sem"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="id_sess"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[SELECT   
    an.ID_ANONYMAT 
FROM   
    ANONYMAT an  
WHERE  
    an.ID_REGLE_ANONYMAT=:id_r  
AND  
    an.ID_ANNEES_UNIVERS=:id_an  
AND  
    an.ID_NIVEAU_FORMATION_PARCOURS=:id_nfp  
AND  
    an.ID_SEMESTRE=:id_sem  
AND  
    an.ID_SESSION=:id_sess]]></SQLQuery>
  <ViewAttribute
    Name="IdAnonymat"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Long"
    ColumnType="LONGINT"
    AliasName="ID_ANONYMAT"
    Expression="ID_ANONYMAT"
    SQLType="NUMERIC"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
