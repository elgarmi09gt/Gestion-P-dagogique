  <SQLQuery><![CDATA[Select fec.code_filiere_ue_semestre_ec ,code_filiere_ue_semestre,fec.coefficient,fec.moyenne_validation,                         
fec.moyenne_eliminatoire,fec.pourcentage_cc,fec.pourcentage_ct,fec.codification_ec,         
fec.heure_cm,fec.heure_tp,fec.heure_td,fec.heure_stage,fec.heure_tpe,fec.responsable Responsable_id, sec.Lib_long_ec,sec.code_ec,                       
snec.lib_nature_ec, snec.code_nature_ec ,prof.code_matricule,prof.emailucad ,prof.prenoms, prof.prenoms || ' ' || prof.nom Responsable,code_etablissement         
from SCOL_FILIERE_UE_SEMESTRE_EC@scolProdref fec         
JOIN SCOL_EC@scolProdref sec ON fec.code_ec=sec.code_ec        
JOIN SCOL_NATURE_EC@scolProdref snec ON fec.code_nature_ec=snec.code_nature_ec        
Left JOIN (      
  Select distinct prf.code_matricule,ag.emailucad, prf.prenoms, prf.nom, code_etablissement, prof_id FROM PROFESSEURS@scolProdref prf       
  JOIN PROFESSEURS_ETABLISSEMENTS@scolProdref prfe On prf.prof_id=prfe.no_emp     
  JOIN v_agents@GRH_PAIE_LINK ag ON prf.code_matricule=ag.code_matricule    
  where prf.code_matricule is not null      
  ) prof On fec.responsable=prof.prof_id]]></SQLQuery>
