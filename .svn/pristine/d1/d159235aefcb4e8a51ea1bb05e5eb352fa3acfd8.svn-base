  <SQLQuery><![CDATA[select p.id_personne,etu.ID_ETUDIANT,etu.numero, p.nom , p.prenoms , p.date_naissance , p.lieu_naissance ,p.adresse  , p.email_personnel  , p.piece_identification   
from inscriptions_admin ia      
join inscriptions_pedagogiques ip on ia.id_inscription_admin = ip.id_inscription_admin      
join ETUDIANTS etu on etu.ID_ETUDIANT=ia.ID_ETUDIANT                      
join PERSONNES p on p.ID_PERSONNE=etu.ID_PERSONNE  
join autorises a on a.id_personne = p.id_personne and ip.id_parcours_maquet_annee = a.id_parcours_maquet_annee 
where ip.id_parcours_maquet_annee=:id_parc_maquette and ia.ID_ANNEES_UNIVERS=:id_annee]]></SQLQuery>
