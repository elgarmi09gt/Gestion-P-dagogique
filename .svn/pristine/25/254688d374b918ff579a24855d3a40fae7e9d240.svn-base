            opinspedSem.getParamsMap().put("id_parc",  parc);
            opinspedSem.getParamsMap().put("id_sem",  les_semestre.get(id_semestre));  
            opinspedSem.getParamsMap().put("idpers",  pers); 
            opinspedSem.execute();

