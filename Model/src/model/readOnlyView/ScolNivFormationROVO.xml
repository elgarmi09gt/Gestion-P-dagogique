  <SQLQuery><![CDATA[SELECT DISTINCT ns.CODE_NIV_SEC,ns.LIBELLE_LONG,ns.LIBELLE_COURT,ns.A_SELECTION,ns.AUTORISATION_PERMISE,ns.PRESENTIELLE,ns.NBRE_INS_PERMISES,ns.DIPLOMANTE,fa.CODE_FORMATION,        
        ns.OUVERT,fr.CODE_ED  CODE_EDOC,ns.UTILISE_EVALUATION,ns.TAG_SEMESTRIALISATION,ns.TAG_GROUPEMATIERE,ns.NIVEAU_CODE,crt.code,crt.nombre_etudiant,crt.actif        
FROM NIVEAUX_SECTIONS@scolProdref ns         
join  NIVEAUX_SECTION_FORMATIONS@scolProdref nsf ON ns.CODE_NIV_SEC = nsf.CODE_NIV_SEC          
join Formation@scolProdref fr ON nsf.CODE_FORMATION = fr.CODE_FORMATION         
join FORMATION_ACC_FORMATION@scolProdref faf ON faf.ID_FORMATION = fr.CODE_FORMATION         
join FORMATION_ACCREDITE@scolProdref fa ON faf.ID_FORMATION_ACC = fa.CODE_FORMATION        
LEFT JOIN (        
    Select crt.code_niv_sec,nombre_etudiant,actif,tc.code        
    from COHORTE@scolProdref crt join TYPE_COHORTE@scolProdref tc on crt.type_cohorte=tc.code) crt        
    ON crt.code_niv_sec=ns.CODE_NIV_SEC 
ORDER BY ns.LIBELLE_LONG]]></SQLQuery>
