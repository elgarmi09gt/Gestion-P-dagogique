    Type="java.lang.Long"/>
  <SQLQuery><![CDATA[SELECT DISTINCT         
    etu.ID_ETUDIANT,         
    etu.NUMERO,         
    pers.PRENOMS,         
    pers.NOM,         
    pers.DATE_NAISSANCE,         
    pers.LIEU_NAISSANCE,         
    ra.CREDIT,         
    ra.RESULTAT,         
    nf.DIPLOMANTE,      
    ra.VALIDE,     
    ra.CHARGER_SCOL,     
    Decode(vda.ANNEE_CLOTUREE, null, 'I', vda.ANNEE_CLOTUREE) ANNEE_CLOTUREE, 
    vda.etat 
FROM         
    (((((((INSCRIPTIONS_PEDAGOGIQUES ip       
    JOIN RESULTATS_ANNUELS ra ON ip.ID_INSCRIPTION_PEDAGOGIQUE = ra.ID_INSCRIPTION_PEDAGOGIQUE)       
    JOIN INSCRIPTIONS_ADMIN ia ON ip.ID_INSCRIPTION_ADMIN = ia.ID_INSCRIPTION_ADMIN)       
    JOIN ETUDIANTS etu ON ia.ID_ETUDIANT = etu.ID_ETUDIANT)       
    JOIN PERSONNES pers ON etu.ID_PERSONNE = pers.ID_PERSONNE)       
    JOIN PARCOURS_MAQUETTE_ANNEE pm ON pm.ID_PARCOURS_MAQUET_ANNEE = ip.ID_PARCOURS_MAQUET_ANNEE and pm.ID_ANNEES_UNIVERS = ia.ID_ANNEES_UNIVERS)       
    JOIN NIVEAUX_FORMATION_PARCOURS nfp ON nfp.ID_NIVEAU_FORMATION_PARCOURS = pm.ID_NIVEAU_FORMATION_PARCOURS)       
    JOIN NIVEAU_FORMATION_COHORTES nfc ON nfp.ID_NIVEAU_FORMATION_COHORTE = nfc.ID_NIVEAU_FORMATION_COHORTE)       
    JOIN NIVEAUX_FORMATIONS nf ON nfc.ID_NIVEAU_FORMATION = nf.ID_NIVEAU_FORMATION      
    LEFT JOIN VALIDATION_DELIB_ANNUEL vda on pm.ID_PARCOURS_MAQUET_ANNEE = vda.ID_PARCOURS_MAQUET_ANNEE and vda.ID_SESSION = :sess      
WHERE     
    pm.ID_PARCOURS_MAQUET_ANNEE = :parcours_maq AND ia.ID_ANNEES_UNIVERS = :annee AND etu.NUMERO = :numero AND ip.ETAT_INSCR_ETAT_INSCR_ID = 22]]></SQLQuery>
